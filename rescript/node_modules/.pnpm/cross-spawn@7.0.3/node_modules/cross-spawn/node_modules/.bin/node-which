#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/which@2.0.2/node_modules/which/bin/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/which@2.0.2/node_modules/which/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/which@2.0.2/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/cross-spawn@7.0.3/node_modules/cross-spawn/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/cross-spawn@7.0.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../which@2.0.2/node_modules/which/bin/node-which" "$@"
else
  exec node  "$basedir/../../../../../which@2.0.2/node_modules/which/bin/node-which" "$@"
fi
