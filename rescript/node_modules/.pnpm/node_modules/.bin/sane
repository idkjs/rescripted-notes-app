#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/sane@4.1.0/node_modules/sane/src/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/sane@4.1.0/node_modules/sane/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/sane@4.1.0/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules/sane/src/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules/sane/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sane/src/cli.js" "$@"
else
  exec node  "$basedir/../sane/src/cli.js" "$@"
fi
