#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/parser@7.13.12/node_modules/@babel/parser/bin/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/parser@7.13.12/node_modules/@babel/parser/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/parser@7.13.12/node_modules/@babel/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/parser@7.13.12/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules/@babel/parser/bin/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules/@babel/parser/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
