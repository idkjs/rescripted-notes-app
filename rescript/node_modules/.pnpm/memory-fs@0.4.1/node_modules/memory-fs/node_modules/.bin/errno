#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/errno@0.1.8/node_modules/errno/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/errno@0.1.8/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/memory-fs@0.4.1/node_modules/memory-fs/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/memory-fs@0.4.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../errno@0.1.8/node_modules/errno/cli.js" "$@"
else
  exec node  "$basedir/../../../../../errno@0.1.8/node_modules/errno/cli.js" "$@"
fi
