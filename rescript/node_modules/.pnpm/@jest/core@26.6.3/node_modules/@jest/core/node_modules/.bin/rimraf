#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/rimraf@3.0.2/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@jest/core@26.6.3/node_modules/@jest/core/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@jest/core@26.6.3/node_modules/@jest/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@jest/core@26.6.3/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@jest/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../../rimraf@3.0.2/node_modules/rimraf/bin.js" "$@"
fi
