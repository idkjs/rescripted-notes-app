#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/browserslist@4.16.3/node_modules/browserslist/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/browserslist@4.16.3/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/helper-compilation-targets@7.13.10_@babel+core@7.13.10/node_modules/@babel/helper-compilation-targets/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/helper-compilation-targets@7.13.10_@babel+core@7.13.10/node_modules/@babel/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/helper-compilation-targets@7.13.10_@babel+core@7.13.10/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../browserslist@4.16.3/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../../browserslist@4.16.3/node_modules/browserslist/cli.js" "$@"
fi
