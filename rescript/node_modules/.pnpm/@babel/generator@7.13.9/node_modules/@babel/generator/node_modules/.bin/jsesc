#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/bin/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/jsesc@2.5.2/node_modules/jsesc/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/jsesc@2.5.2/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/generator@7.13.9/node_modules/@babel/generator/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/generator@7.13.9/node_modules/@babel/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/generator@7.13.9/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../jsesc@2.5.2/node_modules/jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../../../../../../../jsesc@2.5.2/node_modules/jsesc/bin/jsesc" "$@"
fi
