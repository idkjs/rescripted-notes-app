#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/semver@6.3.0/node_modules/semver/bin/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/semver@6.3.0/node_modules/semver/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/semver@6.3.0/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/istanbul-lib-instrument@4.0.3/node_modules/istanbul-lib-instrument/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/istanbul-lib-instrument@4.0.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../semver@6.3.0/node_modules/semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../../../../../semver@6.3.0/node_modules/semver/bin/semver.js" "$@"
fi
