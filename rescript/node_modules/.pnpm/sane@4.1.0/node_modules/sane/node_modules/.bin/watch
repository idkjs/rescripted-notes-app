#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@cnakazawa/watch@1.0.4/node_modules/@cnakazawa/watch/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@cnakazawa/watch@1.0.4/node_modules/@cnakazawa/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@cnakazawa/watch@1.0.4/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@cnakazawa/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/sane@4.1.0/node_modules/sane/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/sane@4.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@cnakazawa/watch@1.0.4/node_modules/@cnakazawa/watch/cli.js" "$@"
else
  exec node  "$basedir/../../../../../@cnakazawa/watch@1.0.4/node_modules/@cnakazawa/watch/cli.js" "$@"
fi
