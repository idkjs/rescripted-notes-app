#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/acorn@4.0.13/node_modules/acorn/bin/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/acorn@4.0.13/node_modules/acorn/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/acorn@4.0.13/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/acorn-dynamic-import@2.0.2/node_modules/acorn-dynamic-import/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/acorn-dynamic-import@2.0.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@4.0.13/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@4.0.13/node_modules/acorn/bin/acorn" "$@"
fi
