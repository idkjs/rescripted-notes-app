#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/is-ci@2.0.0/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/jest-cli@26.6.3/node_modules/jest-cli/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/jest-cli@26.6.3/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
else
  exec node  "$basedir/../../../../../is-ci@2.0.0/node_modules/is-ci/bin.js" "$@"
fi
