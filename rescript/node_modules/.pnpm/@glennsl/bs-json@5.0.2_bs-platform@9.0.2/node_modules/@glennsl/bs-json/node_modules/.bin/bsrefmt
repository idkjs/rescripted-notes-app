#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/bs-platform@9.0.2/node_modules/bs-platform/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/bs-platform@9.0.2/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules:/Volumes/SSD/Github/reason-notes-app/node_modules:/Volumes/SSD/Github/node_modules:/Volumes/SSD/node_modules:/Volumes/node_modules:/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@glennsl/bs-json@5.0.2_bs-platform@9.0.2/node_modules/@glennsl/bs-json/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@glennsl/bs-json@5.0.2_bs-platform@9.0.2/node_modules/@glennsl/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@glennsl/bs-json@5.0.2_bs-platform@9.0.2/node_modules:/Volumes/SSD/Github/reason-notes-app/rescript/node_modules/.pnpm/@glennsl/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../../bs-platform@9.0.2/node_modules/bs-platform/bsrefmt" "$@"
else
  exec node  "$basedir/../../../../../../../bs-platform@9.0.2/node_modules/bs-platform/bsrefmt" "$@"
fi
